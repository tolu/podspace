{
  "compilerOptions": {
      "allowJs": true,
      "checkJs": true,
      "lib": ["es2017", "dom"],
      "module": "esnext",
      "noImplicitAny": false,
      "strictNullChecks": false,
      "strict": true,
      "target": "es2016",
      "typeRoots": [
        "typings"
      ]
  },
  "include": [
    "./js/**/*.js",
    "./js/**/*.ts",
    "./typings/**/*.ts"
  ]
}
