{
  "compilerOptions": {
      "allowJs": true,
      "checkJs": true,
      "lib": ["es2017", "dom"],
      "module": "esnext",
      "noImplicitAny": false,
      "strictNullChecks": false,
      "strict": true,
      "target": "es2016",
      "typeRoots": [
        "typings"
      ],
      "outDir": "./dist",
      "newLine": "LF"
  },
  "include": [
    "./src/**/*.ts",
    "./typings/**/*.d.ts"
  ]
}
